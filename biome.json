{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"vcs": {
		"enabled": false,
		"clientKind": "git",
		"useIgnoreFile": false
	},
	"files": {
		"ignoreUnknown": false,
		"ignore": [
			"node_modules",
			"dist",
			"coverage",
			"build",
			"out",
			"lib",
			"temp",
			"tmp",
			".vscode",
			".idea",
			".DS_Store",
			".env",
			".env.local",
			".env.development",
			".env.test",
			".env.production",
			".env.*.local",
			"npm-debug.log",
			"yarn-debug.log",
			"yarn-error.log",
			"package-lock.json",
			"yarn.lock",
			".pnp.*",
			".pnp.js",
			".pnp.cjs",
			".pnp.ts",
			".pnp.tsx",
			".pnp.jsx",
			".pnp.json",
			".pnp.yaml",
			".pnp.yml",
			".pnp.lockfile",
			"tsconfig.tsbuildinfo",
			".eslintcache",
			".cache",
			".parcel-cache"
		]
	},
	"formatter": {
		"enabled": true,
		"indentStyle": "tab",
		"formatWithErrors": true
	},
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"style": {
				"useImportType": "off"
			}
		}
	},
	"javascript": {
		"formatter": {
			"quoteStyle": "double"
		},
		"parser": {
			"unsafeParameterDecoratorsEnabled": true
		}
	}
}
